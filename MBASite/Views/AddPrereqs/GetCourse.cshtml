@model IEnumerable<MBASite.Models.Course>

@{
    ViewBag.Title = "GetCourse";
}

<h2>GetCourse</h2>

<p>
    @Html.ActionLink("Create New", "AddCourse", "AddCourse")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model=>model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CourseNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PreqId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PreqId)
        </td>
        <td>
            @Html.ActionLink("AddPrereqs", "AddPrereqs", new { id=item.Id })
        </td>
    </tr>
}

</table>
